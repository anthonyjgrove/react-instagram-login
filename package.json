{
  "name": "react-instagram-login",
  "version": "1.4.0",
  "_inBundle": false,
  "_location": "/react-instagram-login",
  "description": "An Instagram Login Component for React",
  "main": "dist/instagram-login.js",
  "bundleDependencies": false,
  "scripts": {
    "start": "set NODE_ENV=development&& webpack-dev-server --config webpack/webpack.config.dev.js",
    "dev": "npm run start",
    "bundle": "rm -rf dist; set NODE_ENV=production&& webpack -p --config webpack/webpack.config.prod.js",
    "test": "mocha --require @babel/register",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "clean": "rm -rf node_modules dist; rm package-lock.json"
  },
  "pre-commit": [
    "test",
    "lint",
    "bundle"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonyjgrove/react-instagram-login.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "instagram-login",
    "instagram-oAuth2",
    "instagram-oAuth"
  ],
  "author": "Anthony Grove <anthony@th3legend.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anthonyjgrove/react-instagram-login/issues"
  },
  "homepage": "https://github.com/anthonyjgrove/react-instagram-login",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "autoprefixer": "7.1.4",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.9",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chai": "^4.1.2",
    "chai-jquery": "2.0.0",
    "eslint-config-ag": "1.0.4",
    "jquery": "3.4.1",
    "jsdom": "9.2.0",
    "mocha": "^7.1.0",
    "pre-commit": "1.2.2",
    "prop-types": "15.6.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "15.1.0",
    "react-dom": "^16.12.0",
    "react-fontawesome": "1.6.1",
    "uglifyjs-webpack-plugin": "0.4.6",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "directories": {
    "test": "test"
  }
}
